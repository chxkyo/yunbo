<modal show="{{showModal}}" bindmodalclose="modalClose" >
  <view slot="modal-title">
    <text>选择日期</text>
  </view>
  <view slot="middle-content">
    <picker value="{{startDate}}" bindchange="bindYearChange" mode="date" fields="year">
      <view class="modal-picker-item">
        <view class="modal-picker-item-des">
          <text>选择年</text>
        </view>
        <view class="modal-picker-right-box ell">
          <view class="time-box">
            <text wx:if="{{chooseEndDate}}">{{endDate}}</text>
          </view>
          <view class="modal-picker-choose-icon">
            <image src="../../images/img/to.png" class="modal-picker-choose-icon"></image>
          </view>
        </view>
      </view>
    </picker>
  </view>
  <button slot="footer" class="addsuisinmebtn confirm-btn" type="primary" hover-class="none" bindtap="saveDate">查询</button>
</modal>
<view class="{{noscroll?'noscroll':''}}">
  <view class="salesstatistics-time" bindtap='modalShow'>{{showYear}}</view>
  <view class="salesstatistics-wrap {{noscroll?'noscroll':''}}">
    <view class="shuju-wrap pd30">
      <view class="shuju-inner-wrap">
          <view class="shuju-header">
            <view><text>商户名称</text></view>
            <view class="goods-name"><text>{{shopName}}</text></view>
          </view>
        <view class="print-time-wrap">
          <view class="print-detail-item">
            <view><text>开始日期：{{startDate}}</text></view>
          </view>
          <view class="print-detail-item">
            <view><text>结束日期：{{endDate}}</text></view>
          </view>
        </view>
        <view class="print-detail-wrap">
          <view class="print-detail-item-wrap">
            <view class="print-detail-item">
              <view>
                <text>订单总数：{{orderCount}}</text>
              </view>
            </view>
          </view>
          <view class="print-detail-item-wrap" hidden='{{categoryList.length == 0 }}'>
            <view class="print-detail-item">
              <view>
                <text>统计类型</text>
              </view>
            </view>
            <view class="print-detail-item" wx:for="{{categoryList}}" wx:key="{{categoryId}}">
              <view>
                <text>{{item.categoryName}}</text>
              </view>
              <view>
                <text>{{item.amount}}元</text>
              </view>
              <view>
                <text>{{item.amountRate}}%</text>
              </view>
            </view>
          </view>
          <view class="print-detail-item-wrap">
            <view class="print-detail-item">
              <view><text>未结账:{{income.acount.unpay_amount}}</text></view>
            </view>
            <view class="print-detail-item">
              <view><text>毛收入:{{income.acount.total_amount}}</text></view>
            </view>
            <view class="print-detail-item">
              <view><text>实际收入:{{income.acount.fact_amount}}</text></view>
            </view>
          </view>
          <view class="print-detail-item-wrap" hidden='{{income.duty.length == 0}}'>
            <view class="print-detail-item">
              <view>
                <text>各班次营业账单</text>
              </view>
            </view>
            <view class="print-detail-item" wx:for="{{income.account.duty}}" wx:key="{{id}}">
              <view>
                <text>{{item.name}}</text>
              </view>
              <view>
                <text>{{item.amount}}元</text>
              </view>
            </view>
          </view>
        </view>
        <view class="print-bottom-wrap">
          <view class="print-detail-item">
            <view><text>营业明细</text></view>
          </view>
          <view class="print-detail-item">
            <view><text>实收总计:{{income.acount.fact_amount}}</text></view>
          </view>
          <view class="print-detail-item">
            <view><text>实收返款合计:{{backFee[0].backFeeAmount}}</text></view>
          </view>
          <view class="print-detail-item">
            <view><text>实收不返款合计:{{backFee[0].unBackFeeAmount}}</text></view>
          </view>
        </view>
        <view class="print-bottom-wrap" hidden='{{receipts.length == 0}}'>
          <view class="print-detail-item">
            <view><text>实收</text></view>
          </view>
          <view class="print-detail-item" wx:for="{{receipts}}" wx:key="{{payType}}">
            <view><text>{{item.payName}}({{item.payCount}}笔)</text></view>
            <view><text>{{item.amount}}元</text></view>
            <view><text>{{item.rate}}%</text></view>
          </view>
        </view>
        <view class="print-bottom-wrap" hidden='{{nonReceipts.length == 0}}'>
          <view class="print-detail-item">
            <view><text>非实收</text></view>
          </view>
          <view class="print-detail-item" wx:for="{{nonReceipts}}" wx:key="{{payType}}">
            <view><text>{{item.payName}}({{item.payCount}}笔)</text></view>
            <view><text>{{item.amount}}元</text></view>
            <view><text></text></view>
          </view>
        </view>
      </view>
    </view>
  </view>
</view>
