/* pages/addfeedback/addfeedback.wxss */
page{
  background: #f5f5f5;
}
textarea{
  width: 100%;
  height: 420rpx;
  padding: 30rpx;
  background: #fff;
  font-size: 30rpx;
}
.tips{
  font-size: 30rpx;
  padding: 30rpx;
  text-align: right;
}
.submit{
  width: 90%;
  height: 100rpx;
  border-radius: 100rpx;
  margin: 0 auto;
  background: #e71a28;
  color: #fff;
  display: flex;
  justify-content: center;
  align-items: center;
}
.addFeedBackSucc{
  width: 100%;
  height: 100vh;
  position: fixed;
  top: 0;
  left: 0;
  background: #f5f5f5;
  z-index: 1;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
}
.addFeedBackSucc image{
  width: 240rpx;
  height: 240rpx;
  margin-bottom: 40rpx;
}
.addFeedBackSucc-text1{
  font-size: 30rpx;
  color: #333;
  margin-bottom: 30rpx;
}
.addFeedBackSucc-text2{
  font-size: 28rpx;
  color: #999;
}