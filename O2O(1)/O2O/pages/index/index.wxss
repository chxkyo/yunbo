/* pages/index/index.wxss */
page{
  background: #f5f5f5;
}
.banner{
  width: 100%;
  height: 500rpx;
}
swiper{
  width: 100%;
  height: 400rpx;
}
.slide-image{
  width: 100%;
  height: 400rpx;
}
.menu{
  width: 92%;
  padding: 40rpx 0 0 0;
  box-sizing: border-box;
  display: flex;
  justify-content: space-around;
  align-items: center;
  flex-wrap: wrap;
  margin: 0 auto;
  background: #fff;
  border-radius: 10rpx;
  transform: translateY(-30rpx);
  box-shadow: 0 2rpx 2rpx 0 rgba(0,0,0,0.1);
}
.menu-item{
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  margin-bottom: 40rpx;
}
.menu-item image{
  width: 80rpx;
  height: 80rpx;
  margin-bottom: 25rpx;
}
.menu-item text{
  font-size: 28rpx;
}
.width25{
  width: 25%;
}
.width33{
  width:33.33%;
}

.con1{
  width: 92%; 
  margin: 0 auto;
}
.con1-head{
  width: 100%;
  display: flex;
  justify-content: space-between;
  align-items: flex-end;
  margin-bottom: 25rpx;
}
.con1-head-left{
  width: 192rpx;
  height: 66rpx;
  background: url(data:image/png;base64,);
  background-size: 192rpx 66rpx;
  font-size: 40rpx;
  font-weight: bold;
  color: #333;
  display: flex;
  justify-content: flex-start;
  align-items: flex-end;
}
.con1-head-left text{
  transform: translateY(-6rpx);
}
.con1-head-right{
  font-size: 28rpx;
  color: #333;
  display: flex;
  justify-content: center;
  align-items: center;
}
.con1-head-right image{
  width: 30rpx;
  height: 30rpx;
  margin-left: 18rpx;
}
.con1-foot{
  width: 100%;
}
.con1-foot-item{
  width: 100%;
  margin-bottom: 20rpx;
  background: #fff;
  border-radius: 10rpx;
  overflow: hidden;
}
.con1-foot-item-img{
  width: 100%;
  height: 230rpx;
}
.con1-foot-item-img image{
  width: 100%;
  height: 100%;
}
.con1-foot-item-text{
  padding: 30rpx 15rpx;
  box-sizing: border-box;
  font-size: 28rpx;
  color: #333;
  font-weight: bold;
}

.con2{
  width: 92%; 
  margin: 0 auto;
}
.con2-head{
  width: 100%;
  display: flex;
  justify-content: space-between;
  align-items: flex-end;
  margin-bottom: 25rpx;
}
.con2-head-left{
  width: 192rpx;
  height: 66rpx;
  background: url(data:image/png;base64,);
  background-size: 192rpx 66rpx;
  font-size: 40rpx;
  font-weight: bold;
  color: #333;
  display: flex;
  justify-content: flex-start;
  align-items: flex-end;
}
.con2-head-left text{
  transform: translateY(6rpx);
}
.con2-head-right{
  font-size: 28rpx;
  color: #333;
  display: flex;
  justify-content: center;
  align-items: center;
}
.con2-head-right image{
  width: 30rpx;
  height: 30rpx;
  margin-left: 18rpx;
}
.con2-foot{
  width: 100%;
  display: flex;
  justify-content: flex-start;
  align-items: flex-start;
  flex-wrap: wrap;
}
.con2-foot-item{
  width: 50%;
  padding: 20rpx;
  box-sizing: border-box;
  margin-bottom: 20rpx;
  background: #fff;
  overflow: hidden;
}
.con2-foot-item:nth-child(2n+1){
  border-right: 1rpx solid #f5f5f5;
  border-top-left-radius: 10rpx;
  border-bottom-left-radius: 10rpx;
}
.con2-foot-item:nth-child(2n){
  border-right: none;
  border-top-right-radius: 10rpx;
  border-bottom-right-radius: 10rpx;
}
.con2-foot-item-img{
  width: 100%;
  height: 250rpx;
  display: flex;
  justify-content: center;
  align-items: flex-start;
}
.con2-foot-item-img image{
  width: 300rpx;
  height: 210rpx;
}
.con2-foot-item-title{
  font-size: 28rpx;
  color: #333;
  font-weight: bold;
  margin-bottom: 15rpx;
}
.con2-foot-item-points{
  font-size: 24rpx;
  color: #e72427;
  font-weight: bold;
}