/* pages/mallactivity/applysucc/applysucc.wxss */
page{
  width: 100%;
  height: 100vh;
  background: #f5f5f5;
}
.convertsucc{
  width: 100%;
  height: 600rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  flex-direction: column;
  position: fixed;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  margin: auto;
}
.convertsucc-head{
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
}
.convertsucc-head image{
  width: 240rpx;
  height: 240rpx;
}
.convertsucc-head-title{
  font-size: 30rpx;
  color: #333;
  margin: 40rpx 0 30rpx 0;
}
.convertsucc-head-text{
  font-size: 26rpx;
  color: #999;
}
.convertsucc-foot{
  width: 340rpx;
  height: 100rpx;
  border-radius: 100rpx;
  border: 1rpx solid #999;
  font-size: 30rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}