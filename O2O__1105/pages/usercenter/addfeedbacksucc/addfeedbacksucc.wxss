/* pages/usercenter/addfeedbacksucc/addfeedbacksucc.wxss */
.addFeedBackSucc{
  width: 100%;
  height: 100vh;
  position: fixed;
  top: 0;
  left: 0;
  background: #f5f5f5;
  z-index: 1;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
}
.addFeedBackSucc image{
  width: 240rpx;
  height: 240rpx;
  margin-bottom: 40rpx;
}
.addFeedBackSucc-text1{
  font-size: 30rpx;
  color: #333;
  margin-bottom: 30rpx;
}
.addFeedBackSucc-text2{
  font-size: 28rpx;
  color: #999;
}
.convertsucc-foot{
  width: 340rpx;
  height: 100rpx;
  border-radius: 100rpx;
  border: 1rpx solid #999;
  font-size: 30rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  margin-top: 20px;
}