/* pages/mytickets/mytickets.wxss */
page{
  background: #f5f5f5;
}
.mytickets{
  width: 100%;
  padding: 0 30rpx;
  box-sizing: border-box;
  background: #fff;
}
.mytickets-item{
  width: 100%;
  padding: 35rpx 0;
  box-sizing: border-box;
  border-bottom: 1rpx solid #f5f5f5;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.mytickets-itemLeft{
  width: 120rpx;
  height: 120rpx;
}
.mytickets-itemLeft image{
  width: 100%;
  height: 100%;
}
.mytickets-itemRight{
  font-size: 35rpx;
  font-weight: bold;
}
.mytickets-itemRight image{
  width: 12rpx;
  height: 20rpx;
  margin-left: 20rpx;
}