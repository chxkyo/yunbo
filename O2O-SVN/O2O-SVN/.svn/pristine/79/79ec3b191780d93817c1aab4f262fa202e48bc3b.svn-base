/* pages/mallactivity/questionnaire/questionnaire.wxss */
page{
  width: 100%;
  height: 100vh;
  background: #f5f5f5;
}
.questionnaire{
  padding: 30rpx;
  box-sizing: border-box;
}
.questionnaire-item{
  background: #fff;
  font-size: 32rpx;
  border-radius: 10rpx;
  padding: 30rpx 30rpx 0 30rpx;
  box-sizing: border-box;
  margin-bottom: 30rpx;
}
.questionnaire-item-head{
  margin-bottom: 60rpx;
}
.radio{
  width: 100%;
  padding: 20rpx 0;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-bottom: 1rpx solid #f5f5f5;
}