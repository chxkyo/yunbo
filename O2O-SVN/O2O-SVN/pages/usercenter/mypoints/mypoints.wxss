/* pages/mypoints/mypoints.wxss */
page{
  background: #f5f5f5;
}
.mypoints-head{
  width: 100%;
  height: 300rpx;
  background: url(data:image/jpeg;base64,);
  background-size: 100% 300rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  margin-bottom: 30rpx;
}
.mypoints-head-text1{
  font-size: 30rpx;
  color: #333;
}
.mypoints-head-text2{
  font-size: 140rpx;
  color: #333;
}
.mypoints-con{
  width: 100%;
}
.mypoints-con-top{
  width: 100%;
  height: 100rpx;
  display: flex;
  justify-content: space-around;
  align-items: center;
  background: #fff;
  border-bottom: 1rpx solid #eee;
}
.mypoints-con-top-item{
  position: relative;
  height: 100%;
  line-height: 100rpx;
  color: #999;
}
.active{
  color: #000;
}
.active-line{
  width: 80rpx;
  height: 4rpx;
  background: #e71a28;
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
  margin: auto;
}
.mypoints-con-mid{
  width: 100%;
  height: 95rpx;
  background: #fff;
  border-bottom: 1rpx solid #eee;
  position: relative;
  box-sizing: border-box;
}
.mypoints-con-mid-head{
  width: 100%;
  height: 100%;
  display: flex;
  justify-content: flex-end;
  align-items: center;
  border-bottom: 1rpx solid #eee;
}
.mypoints-con-mid-head-text{
  font-size: 28rpx;
  color: #000;
}
.triangle{
  width: 0;
  height: 0;
  border-width: 6px;
  border-style: solid;
  border-color: #999 transparent transparent transparent;
  transform: translateY(4rpx);
  margin: 0 30rpx 0 20rpx;
}
.rotate180{
  transform: rotate(180deg);
}
.mypoints-con-mid-con{
  width: 100%;
  position: absolute;
  top: 95rpx;
}
.mypoints-con-mid-con-item{
  width: 100%;
  height: 95rpx;
  font-size: 28rpx;
  color: #000;
  background: #fff;
  border-bottom: 1rpx solid #eee;
  display: flex;
  justify-content: center;
  align-items: center;
}
.mypoints-con-foot{
  width: 100%;
  background: #fff;
}
.mypoints-con-foot-item{
  width: 100%;
  padding: 35rpx 30rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-bottom: 1rpx solid #eee;
}
.mypoints-con-foot-itemLeft-title{
  font-size: 34rpx;
  color: #000;
  margin-bottom: 20rpx;
}
.mypoints-con-foot-itemLeft-date{
  font-size: 28rpx;
  color: #999;
}
.mypoints-con-foot-itemRight{
  font-size: 40rpx;
  color: #e71a28;
}
.pointsTasks{
  width: 100%;
  padding: 30rpx;
  box-sizing: border-box;
}
.pointsTasks-item{
  width: 100%;
  padding: 20rpx 30rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
  align-items: center;
  background: #fff;
  border-radius: 10rpx;
  margin-bottom: 30rpx;
}
.pointsTasks-itemLeft-top{
  font-size: 28rpx;
  color: #333;
  margin-bottom: 35rpx;
}
.pointsTasks-itemLeft-bot{
  font-size: 22rpx;
  color: #999;
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
.pointsTasks-icon1{
  width: 34rpx;
  height: 34rpx;
  background: url(data:image/jpeg;base64,);
  background-size: 34rpx 34rpx;
  margin-right: 15rpx;
}
.pointsTasks-itemRight{
  font-size: 24rpx;
  color: #333;
  display: flex;
  justify-content: flex-end;
  align-items: center;
}
.pointsTasks-icon2{
  width: 12rpx;
  height: 20rpx;
  background: url(data:image/jpeg;base64,);
  background-size: 12rpx 20rpx;
  margin-left: 20rpx;
}